{
  "expo": {
    "name": "Balancium",
    "slug": "balancium-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#14ba82"
    },
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "light",
      "backgroundColor": "#14ba82",
      "bundleIdentifier": "com.devlucasfontoura.balanciummobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#14ba82"
      },
      "statusBarStyle": "light",
      "statusBarBackgroundColor": "#14ba82",
      "navigationBar": {
        "backgroundColor": "#14ba82"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.devlucasfontoura.balanciummobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "664097fb-0e4a-4a9e-8087-7896ae814847"
      }
    }
  }
}
